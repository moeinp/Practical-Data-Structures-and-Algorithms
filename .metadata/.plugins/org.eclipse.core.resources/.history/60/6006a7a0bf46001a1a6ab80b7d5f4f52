package stack;

public class App {

	public static void main(String[] args) {
		Stack myStack = new Stack(10);
		myStack.push(0);
		myStack.push(1);
		myStack.push(2);
		myStack.push(3);
		myStack.push(4);
		myStack.push(5);
		myStack.push(6);
		myStack.push(7);
		myStack.push(8);
		myStack.push(9);
		myStack.push(10);

		while (!myStack.isEmpty()) {
			try {
				int val = myStack.pop();
				System.out.println(val);
			} catch (IndexOutOfBoundsException e) {
				System.out.println("Something went wrong.");
			}
		}

	}

}
