package stack;

public class App {

	public static void main(String[] args) {

		Stack myStack = new Stack(10);

		try {
			for (int i = 0; i <= 10; i++)
				myStack.push(i);
		} catch (IndexOutOfBoundsException e) {
			System.out.println("can not push 10 since stack is alredy full.");

		}



	}

}
